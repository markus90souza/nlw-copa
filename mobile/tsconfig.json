{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@types/*":[ "./src/@types/*"],
      "@assets/*":[ "./src/assets/*"],
      "@components/*":[ "./src/components/*"],
      "@routes/*":[ "./src/routes/*"],
      "@screens/*":[ "./src/screens/*"],
      "@storage/*":[ "./src/storage/*"],
      "@theme/*":[ "src/theme/*"],
      "@hooks/*":[ "./src/hooks/*"],
      "@contexts/*":[ "./src/contexts/*"],
      "@services/*":[ "./src/services/*"],
    }
  }
}
